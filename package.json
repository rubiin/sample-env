{
  "name": "sample-env",
  "type": "module",
  "version": "1.0.10",
  "packageManager": "pnpm@7.4.0",
  "description": "Generate sample file from .env for your apps",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "funding": "https://github.com/sponsors/rubiin",
  "homepage": "https://github.com/rubiin/sample-env#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/sample-env.git"
  },
  "bugs": {
    "url": "https://github.com/rubiin/sample-env/issues"
  },
  "keywords": [
    "cli",
    "env",
    "sample",
    "dotenv"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "sample-env": "./bin/sample-env.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint 'src/**/*.ts' --cache",
    "lint:fix": "eslint 'src/**/*.ts' --cache --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rubiin/eslint-config-typescript": "^1.4.0",
    "@rubiin/tsconfig": "^1.0.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.6.1",
    "@types/yargs": "^17.0.24",
    "bumpp": "^9.2.0",
    "eslint": "^8.49.0",
    "pnpm": "^8.7.5",
    "rimraf": "^5.0.1",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
