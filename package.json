{
  "name": "sample-env",
  "type": "module",
  "version": "2.1.0",
  "description": "Generate sample file from .env for your apps",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "funding": "https://github.com/sponsors/rubiin",
  "homepage": "https://github.com/rubiin/sample-env#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/sample-env.git"
  },
  "bugs": {
    "url": "https://github.com/rubiin/sample-env/issues"
  },
  "keywords": [
    "cli",
    "env",
    "sample",
    "dotenv"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "sample-env": "./bin/sample-env.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint 'src/**/*.ts' --cache",
    "lint:fix": "eslint 'src/**/*.ts' --cache --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rubiin/eslint-config": "^1.9.16",
    "@rubiin/tsconfig": "^1.1.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.0",
    "@types/yargs": "^17.0.32",
    "bumpp": "^9.4.1",
    "eslint": "^9.4.0",
    "pnpm": "^9.1.4",
    "typescript": "5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
}
