{
  "name": "sample-env",
  "type": "module",
  "version": "2.1.2",
  "description": "Generate sample file from .env for your apps",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "funding": "https://github.com/sponsors/rubiin",
  "homepage": "https://github.com/rubiin/sample-env#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/sample-env.git"
  },
  "bugs": {
    "url": "https://github.com/rubiin/sample-env/issues"
  },
  "keywords": [
    "cli",
    "env",
    "sample",
    "dotenv"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "sample-env": "./bin/sample-env.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rubiin/tsconfig": "^1.2.1",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.18.6",
    "@types/yargs": "^17.0.33",
    "bumpp": "^10.2.3",
    "oxlint": "^1.18.0",
    "pnpm": "^10.17.1",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
