{
	"extends": "@rubiin/tsconfig",
	"compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "lib": ["esnext"],
		"baseUrl": "src",
		"outDir": "dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": false
	},
	"include": ["test/**/*", "src/**/*",".eslintrc.cjs","build.config.ts"],
	"exclude": ["node_modules", "dist"],
}
